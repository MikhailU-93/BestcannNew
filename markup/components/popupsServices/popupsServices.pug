mixin popupsServices(data)
  section.popupsServices       
    each item, index in data.items
      .popupsServices_item.popup-item(id =`popup-service-${index}`)
        img.popupsServices_photo(src= item.photo, alt="")
        span.popupsServices_infoGroup
          h3.popupsServices_title= item.name
          span.popupsServices_text= item.more