mixin buyAuto(data)
  section.buyAuto
    .buyAuto_wrap
      h2.buyAuto_title.title.animateEl Купить авто с нами легко
      .buyAuto_items
        each item in data.items
          .buyAuto_item.animateEl
            img.buyAuto_icon(src= item.icon, alt="")
            span.buyAuto_info.subtitle= item.info