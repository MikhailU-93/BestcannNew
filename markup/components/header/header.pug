mixin header(data)
  header.header(id="main")
    .header_headLine
      +socialsLine(socialsLine.main)
      
      .header_nav
        i.header_close.toggle-menu-elem
        .header_navWrap
          nav.header_links
            each link, index in data.links
              a.header_navLink.animateEl.js-scroll-to(href= index)= link

      .header_back.toggle-menu-elem